<resources>
    <style name="Theme.FITX06" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/primary_pink</item>
        <item name="colorPrimaryDark">@color/primary_pink_dark</item>
        <item name="colorAccent">@color/primary_pink</item>
        <item name="android:statusBarColor">@color/primary_black</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>

    <style name="Theme.FITX06.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor">@color/primary_black</item>
        <item name="android:navigationBarColor">@color/primary_black</item>
    </style>
</resources>